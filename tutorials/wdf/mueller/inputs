# DEM model settings
#
dem.solve = poppys

dem.friction_coeff.pp     =     0.1
dem.friction_coeff.pw     =     0.1

dem.spring_const.pp       =   110.0
dem.spring_const.pw       =   110.0

dem.restitution_coeff.poppys.poppys  =     0.95
dem.restitution_coeff.poppys.wall    =     0.95

dem.spring_tang_fac.pp    =     0.285714285
dem.spring_tang_fac.pw    =     0.285714285

dem.damping_tang_fac.pp   =     0.5
dem.damping_tang_fac.pw   =     0.5


#! EB geometry
#!-----------------------------------------------------------//
#! none

#_______________________________________________________________________
# Regions for defining ICs and BCs
#

mfix.regions = fluid-only inflow  outflow  bottom-wall  top-wall  front-wall  back-wall

regions.fluid-only.lo  = 0.0000  0.0000  0.0000
regions.fluid-only.hi  = 0.1760  0.0440  0.0110

regions.inflow.lo      = 0.0000  0.0000  0.0000
regions.inflow.hi      = 0.0000  0.0440  0.0110

regions.outflow.lo     = 0.1760  0.0000  0.0000
regions.outflow.hi     = 0.1760  0.0440  0.0110

regions.bottom-wall.lo = 0.0000  0.0000  0.0000
regions.bottom-wall.hi = 0.1760  0.0000  0.0110

regions.top-wall.lo    = 0.0000  0.0440  0.0000
regions.top-wall.hi    = 0.1760  0.0440  0.0110

regions.front-wall.lo  = 0.0000  0.0000  0.0005
regions.front-wall.hi  = 0.1760  0.0440  0.0005

regions.back-wall.lo   = 0.0000  0.0000  0.0105
regions.back-wall.hi   = 0.1760  0.0440  0.0105


#_______________________________________________________________________
# Initial Conditions
#

ic.regions = fluid-only


# Background IC covering the full domain with gas.
#---------------------------------------------------------------------//
ic.fluid-only.fluid.volfrac   = 1.0

ic.fluid-only.fluid.velocity  = 0.0  0.0  0.0

#_______________________________________________________________________
# Boundary Conditions
# U/Umf                       =  2     3
# bc.inflow.fluid.velocity(1) = 0.60  0.90
#
bc.regions = inflow  outflow  bottom-wall  top-wall  front-wall  back-wall

bc.inflow = mi
bc.inflow.fluid.volfrac  =  1.0
bc.inflow.fluid.velocity =  0.60  0.0  0.0

bc.outflow = po
bc.outflow.fluid.pressure =  0.0

bc.bottom-wall = nsw
bc.top-wall    = nsw
bc.front-wall  = nsw
bc.back-wall   = nsw

bc.bottom-wall.normal =  0.0  1.0  0.0
bc.top-wall.normal    =  0.0 -1.0  0.0
bc.front-wall.normal  =  0.0  0.0  1.0
bc.back-wall.normal   =  0.0  0.0 -1.0
