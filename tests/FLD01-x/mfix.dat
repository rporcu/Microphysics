########################################################################
# Horizontal channel (rectangular plane Poiseuille flow)               #
#                                                                      #
# Author: Aniruddha Choudhary                           Date: Jan 2015 #
#                                                                      #
#                                                                      #
########################################################################

  RUN_NAME = 'FLD01'
  DESCRIPTION = 'Channel Flow'

#_______________________________________________________________________
# RUN CONTROL SECTION

  RUN_TYPE = 'NEW'

  GRAVITY(1:3) = 3*0.0

  CALL_USR = .T.

#_______________________________________________________________________
# NUMERICAL SECTION

  MAX_NIT = 200000
  TOL_RESID = 1.0E-10

  NORM_G = 0.0d0

  LEQ_PC = 9*'NONE'

  DISCRETIZE = 9*2

#_______________________________________________________________________
# GEOMETRY SECTION



#_______________________________________________________________________
# GAS-PHASE SECTION

  RO_G0 = 1.0     ! (kg/m3)
  MU_G0 = 1.0d-3  ! (Pa.sec)

#_______________________________________________________________________
# SOLIDS-PHASE SECTION

#_______________________________________________________________________
# INITIAL CONDITIONS SECTION

  IC_X_w(1) =   0.00    ! (m)
  IC_X_e(1) =   0.20    ! (m)
  IC_Y_s(1) =   0.00    ! (m)
  IC_Y_n(1) =   0.01    ! (m)
  IC_Z_b(1) =   0.00    ! (m)
  IC_Z_t(1) =   1.00    ! (m)

  IC_EP_g(1) =  1.0

  IC_P_g(1) = 101.325d3 ! (Pa)

  IC_U_g(1) =  10.0     ! (m/sec)
  IC_V_g(1) =   0.0     ! (m/sec)
  IC_W_g(1) =   0.0     ! (m/sec)

#_______________________________________________________________________
# BOUNDARY CONDITIONS SECTION

! The east and west boundaries are set at cyclic with a specified
! pressure drop (delP_X).
!---------------------------------------------------------------------//
  CYCLIC_X_PD = .T.

  DELP_X = 240.0

! North and south walls are no-slip
!---------------------------------------------------------------------//
  BC_X_w(1:2) =   0.00     0.00  ! (m)
  BC_X_e(1:2) =   0.20     0.20  ! (m)
  BC_Y_s(1:2) =   0.00     0.01  ! (m)
  BC_Y_n(1:2) =   0.00     0.01  ! (m)
  BC_Z_b(1:2) =   0.00     0.00  ! (m)
  BC_Z_t(1:2) =   1.00     1.00  ! (m)

  BC_Type(1:2) = 'NSW'    'NSW'


! Bottom and top walls are free-slip
!---------------------------------------------------------------------//
  BC_X_w(3:4) =   0.00    0.00  ! (m)
  BC_X_e(3:4) =   0.20    0.20  ! (m)
  BC_Y_s(3:4) =   0.00    0.00  ! (m)
  BC_Y_n(3:4) =   0.01    0.01  ! (m)
  BC_Z_b(3:4) =   0.00    1.00  ! (m)
  BC_Z_t(3:4) =   0.00    1.00  ! (m)

  BC_Type(3:4) = 'FSW'    'FSW'

#_______________________________________________________________________
# OUTPUT CONTROL SECTION



  FULL_LOG = .T.

  RESID_STRING  = 'P0', 'U0', 'V0'
