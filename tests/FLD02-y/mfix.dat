#########################################################################
#                                                                       #
# Author: Avinash Vaidheeswaran                         Date: July 2016 #
# Couette flow problem:                                                 #
#                                                                       #
#   The south wall is held stationary and the north wall moves          #
# with a velocity U. Cyclic boundary conditions along x direction with  #
# favorable pressure gradient                                           #
#                                                                       #
#                                                                       #
#########################################################################

  RUN_NAME = 'FLD02'
  DESCRIPTION = 'Developing pipe flow'

#_______________________________________________________________________
# RUN CONTROL SECTION

  GRAVITY(1:3) = 3*0.0

  CALL_USR = .F.
  FULL_LOG = .T.

#_______________________________________________________________________
# GAS-PHASE SECTION

  RO_g0 = 1.00      ! (kg/m3)
  MU_g0 = 1.00d-3   ! (Pa.s)

#_______________________________________________________________________
# INITIAL CONDITIONS SECTION

  IC_X_w(1) =   0.000   ! (m)
  IC_X_e(1) =   0.020   ! (m)
  IC_Z_b(1) =   0.000   ! (m)
  IC_Z_t(1) =   0.020   ! (m)

  IC_Y_s(1) =   0.000   ! (m)
  IC_Y_n(1) =   0.100   ! (m)

  IC_EP_g(1) =  1.000

  IC_P_g(1) = 101.325d3 ! (Pa)

  IC_U_g(1) =   0.000   ! (m/sec)
  IC_V_g(1) =  10.000   ! (m/sec)
  IC_W_g(1) =   0.000   ! (m/sec)

#_______________________________________________________________________
# BOUNDARY CONDITIONS SECTION


! Mass inflow
!---------------------------------------------------------------------//
  BC_X_w(1) =   0.000    ! (m)
  BC_X_e(1) =   0.020    ! (m)
  BC_Z_b(1) =   0.000    ! (m)
  BC_Z_t(1) =   0.020    ! (m)

  BC_Y_s(1) =   0.000    ! (m)
  BC_Y_n(1) =   0.000    ! (m)

  BC_TYPE(1)  = 'MI'
# BC_P_g(1) =   101.325d3

  BC_U_g(1) =    0.00    ! (m/sec)
  BC_V_g(1) =   10.00    ! (m/sec)
  BC_W_g(1) =    0.00    ! (m/sec)


! Pressure outflow
!---------------------------------------------------------------------//
  BC_X_w(2) =   0.000   ! (m)
  BC_X_e(2) =   0.020   ! (m)
  BC_Z_b(2) =   0.000   ! (m)
  BC_Z_t(2) =   0.020   ! (m)

  BC_Y_s(2) =   0.100   ! (m)
  BC_Y_n(2) =   0.100   ! (m)

  BC_TYPE(2) =  'PO'

  BC_P_g(2) = 101.325d3

!---------------------------------------------------------------------//
! Top and bottom walls are no-slip
!---------------------------------------------------------------------//
  BC_X_w(3:4) =   0.000    0.000  ! (m)
  BC_X_e(3:4) =   0.020    0.020  ! (m)
  BC_Z_b(3:4) =   0.000    0.020  ! (m)
  BC_Z_t(3:4) =   0.000    0.020  ! (m)

  BC_Y_s(3:4) =   0.000    0.000  ! (m)
  BC_Y_n(3:4) =   0.100    0.100  ! (m)

  BC_Type(3:4) =   'NSW'     'NSW'

  BC_Center(3,1:3) =   0.010   0.050   0.005
  BC_Center(4,1:3) =   0.010   0.050   0.015

  BC_Normal(3,1:3) =   0.000   0.000   1.000
  BC_Normal(4,1:3) =   0.000   0.000  -1.000


!---------------------------------------------------------------------//
! Top and bottom walls are no-slip
!---------------------------------------------------------------------//
  BC_X_w(5:6) =   0.000    0.020  ! (m)
  BC_X_e(5:6) =   0.000    0.020  ! (m)
  BC_Z_b(5:6) =   0.000    0.000  ! (m)
  BC_Z_t(5:6) =   0.020    0.020  ! (m)

  BC_Y_s(5:6) =   0.000    0.000  ! (m)
  BC_Y_n(5:6) =   0.100    0.100  ! (m)

  BC_Type(5:6) =  'NSW'      'NSW'

  BC_Center(5,1:3) =   0.005   0.050   0.010
  BC_Center(6,1:3) =   0.015   0.050   0.010

  BC_Normal(5,1:3) =   1.000   0.000   0.000
  BC_Normal(6,1:3) =  -1.000   0.000   0.000
